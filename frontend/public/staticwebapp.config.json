{
  "navigationFallback": {
    "rewrite": "/index.html",
    "exclude": ["/assets/*"]
  },
  "routes": [
    {
      "route": "/assets/*.js",
      "headers": {
        "Content-Type": "text/javascript; charset=utf-8"
      }
    },
    {
      "route": "/assets/*.mjs",
      "headers": {
        "Content-Type": "text/javascript; charset=utf-8"
      }
    },
    {
      "route": "/assets/*.css",
      "headers": {
        "Content-Type": "text/css; charset=utf-8"
      }
    },
    {
      "route": "/*.js",
      "headers": {
        "Content-Type": "text/javascript; charset=utf-8"
      }
    }
  ],
  "globalHeaders": {
    "X-Content-Type-Options": "nosniff",
    "X-Frame-Options": "DENY",
    "Content-Security-Policy": "default-src 'self' https:; script-src 'self' 'unsafe-inline' 'unsafe-eval'; style-src 'self' 'unsafe-inline'; img-src 'self' data: https:; font-src 'self' data:; connect-src 'self' https://nba-dfs-backend.azurewebsites.net https://snowflake-ai-foundry-agent.cognitiveservices.azure.com"
  },
  "responseOverrides": {
    "404": {
      "rewrite": "/index.html",
      "statusCode": 200
    }
  }
}
